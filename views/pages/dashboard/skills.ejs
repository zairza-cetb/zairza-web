<style>
  /* Chrome, Safari, Edge, Opera */
  input[type="number"]::-webkit-outer-spin-button,
  input[type="number"]::-webkit-inner-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }
  

  /* Firefox */
  input[type="number"] {
    -moz-appearance: textfield;
  }
</style>
<div class="w-full h-screen overflow-x-auto border-t flex flex-col">
  <main class="w-full flex-grow p-6">
    <div x-data="app()" x-cloak>
      <div class="max-w-3xl mx-auto px-4 py-10">
        <div x-show.transition="step === 'complete'">
          <div
            class="
              bg-white
              rounded-lg
              p-10
              flex
              items-center
              shadow
              justify-between
            "
          >
            <div>
              <svg
                class="mb-4 h-20 w-20 text-green-500 mx-auto"
                viewBox="0 0 20 20"
                fill="currentColor"
              >
                <path
                  fill-rule="evenodd"
                  d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.707-9.293a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z"
                  clip-rule="evenodd"
                />
              </svg>
            

              <h2 class="text-2xl mb-4 text-gray-800 text-center font-bold">
                Registration successful for Skills++
              </h2>

              <div class="text-gray-600 mb-8">
                Thank you for your interest! We have received your registration and have sent you an email with all necessary instructions. All the required communication will be conveyed over mail only.
              </div>

              <a href="https://zairza-website.s3.ap-south-1.amazonaws.com/miscellaneous/Skills%2B%2B+Brochure+(1).pdf"
                target="_blank"
              >
                <button
                  class="
                    block
                    mx-auto
                    focus:outline-none
                    py-2
                    px-5
                    rounded-lg
                    shadow-sm
                    text-center text-gray-600
                    bg-white
                    hover:bg-gray-100
                    font-medium
                    border
                  "
                >
                  Download brochure
                </button>
              </a>
            </div>
          </div>
        </div>
        <% if(isRegistered) {%>
        <div x-show.transition="step = 'complete'">
        <% } else {%>
        <div x-show.transition="step != 'complete'">
        <% } %>
          <!-- Top Navigation -->
          <% if(!isRegistered) {%>
          <div class="border-b-2 py-4">
            <div
              class="
                uppercase
                tracking-wide
                text-xs
                font-bold
                text-gray-500
                mb-1
                leading-tight
              "
              x-text="`Step: ${step} of 2`"
            ></div>
            <div
              class="
                flex flex-col
                md:flex-row
                md:items-center
                md:justify-between
              "
            >
              <div class="flex-1">
                <div x-show="step === 1">
                  <div class="text-lg font-bold text-gray-700 leading-tight">
                    Your skills++ profile
                  </div>
                </div>

                <div x-show="step === 2">
                  <div class="text-lg font-bold text-gray-700 leading-tight">
                    Choose domain for skills++
                  </div>
                </div>
              </div>

              <div class="flex items-center md:w-64">
                <div class="w-full bg-white rounded-full mr-2">
                  <div
                    class="
                      rounded-full
                      bg-green-500
                      text-xs
                      leading-none
                      h-2
                      text-center text-white
                    "
                    :style="'width: '+ parseInt(step / 2 * 100) +'%'"
                  ></div>
                </div>
                <div
                  class="text-xs w-10 text-gray-600"
                  x-text="parseInt(step / 2 * 100) +'%'"
                ></div>
              </div>
            </div>
          </div>
          <% } %>
          <!-- /Top Navigation -->

          <!-- Step Content -->
          <div class="py-10">
            <div x-show.transition.in="step === 1">
              <div class="mb-5 text-center">
                <div
                  class="flex flex-col justify-around mt-10 items-center h-16"
                >
                  <label for="phone" class="font-bold mb-1 text-gray-700 block pb-3"
                    >Enter your mobile number preferablly whatsapp</label
                  >
                  <input
                    name="phone"
                    id="phone"
                    type="number"
                    class="w-1/2 px-4 py-2 rounded-lg font-medium bg-gray-100 border border-gray-200 placeholder-gray-500 text-sm focus:outline-none focus:ring-2 focus:ring-blue-500  text-gray-600 font-medium"
                  />
                </div>
                <% if(!user.registrationNo) {%>
                <div
                  class="flex flex-col justify-around mt-10 items-center h-16"
                >
                  <label for="phone" class="font-bold mb-1 text-gray-700 block pb-3"
                    >Enter your registration number</label
                  >
                  <input
                    name="regNo"
                    id="regNo"
                    type="text"
                    class="w-1/2 px-4 py-2 rounded-lg font-medium bg-gray-100 border border-gray-200 placeholder-gray-500 text-sm focus:outline-none focus:ring-2 focus:ring-blue-500  text-gray-600 font-medium"
                  />
                </div>
                <% } %>
              </div>
              <div class="mb-5">
                <div class="container">
                  <label
                    for="profession"
                    class="font-bold mb-1 text-gray-700 block"
                    >Your skills</label
                  >
  
                  <div class="tag-container">
                    <input name="userSkills" class="tag-input" list="skills-required" placeholder="Add your skills" id="userSkills"/>
                    <datalist id="skills-required">
                      <option value="HTML">HTML</option>
                      <option value="CSS">CSS</option>
                      <option value="Javascript">Javascript</option>
                      <option value="C++">C++</option>
                      <option value="Java">Java</option>
                      <option value="Python">Python</option>
                      <option value="C">C</option>
                      <option value="PHP">PHP</option>
                      <option value="Flutter">Flutter</option>
                      <option value="ReactJS">ReactJS</option>
                      <option value="NodeJS">NodeJS</option>
                      <option value="Android">Android</option>
                      <option value="Kotlin">Kotlin</option>
                      <option value="Graphic Designing">Graphic Designing</option>
                      <option value="Video Editing">Video Editing</option>
                      <option value="UI">UI</option>
                      <option value="Competitive Coding">Competitive Coding</option>
                      <option value="Data Science">Data Science</option>
                      <option value="Machine Learning">Machine Learning</option>
                      <option value="Devops">Devops</option>
                      <option value="Blockchain">Blockchain</option>
                      <option value="Microcontrollers">Microcontrollers</option>
                      <option value="Embedded Systems">Embedded Systems</option>
                      <option value="IOT">IOT</option>
                      <option value="ROS">ROS</option>
                      <option value="Cloud Computing">Cloud Computing</option>
                    </datalist>
                  </div>
                </div>
              </div>
            </div>
            <div x-show.transition.in="step === 2">
              <div class="mb-5">
                <label for="" class="font-bold mb-1 text-gray-700 block text-center"
                  >Domains</label
                >

                <div class="grid md:grid-cols-4 grid-rows-12 gap-x-8 gap-y-3">
                  <% for (domain of domains){ %>
                  <label
                    class="
                      flex
                      justify-start
                      items-center
                      text-truncate
                      rounded-lg
                      bg-white
                      pl-4
                      pr-6
                      py-3
                      shadow-sm
                      "
                  >
                    <div class="text-teal-600 mr-3">
                      <input
                        type="radio"
                        name="domain"
                        value="<%= domain._id  %>"
                        class="
                          form-radio
                          focus:outline-none
                          focus:shadow-outline
                        "
                      />
                    </div>
                    <div class="select-none text-gray-700"><%= domain.name %></div>
                  </label>
                  <% } %>
                </div>
              </div>
            </div>
          </div>
          <!-- / Step Content -->
        </div>
        <!-- Bottom Navigation -->
        <div class="py-5 bg-white" x-show="step != 'complete'">
          <div class="max-w-3xl mx-auto px-4">
            <div class="flex justify-between">
              <div class="w-1/2">
                <button
                  x-show="step > 1"
                  @click="step--"
                  class="
                    w-32
                    focus:outline-none
                    py-2
                    px-5
                    rounded-lg
                    shadow-sm
                    text-center text-gray-600
                    bg-white
                    hover:bg-gray-100
                    font-medium
                    border
                  "
                  id="prevBtn"
                >
                  Previous
                </button>
              </div>

              <div class="w-1/2 text-right">
                <button
                  x-show="step < 2"
                  class="
                    w-40
                    md:w-48
                    inline-flex 
                    justify-center
                    focus:outline-none
                    border border-transparent
                    py-2
                    px-5
                    rounded-lg
                    shadow-sm
                    text-center text-white
                    bg-blue-500
                    hover:bg-blue-600
                    font-medium
                  "
                  id="fakeNextBtn"
                >
                <svg class="animate-spin -ml-1 mr-3 h-5 w-5 text-white hidden" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24">
                  <circle class="opacity-25" cx="12" cy="12" r="10" stroke="currentColor" stroke-width="4"></circle>
                  <path class="opacity-75" fill="currentColor" d="M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z"></path>
                </svg>
                <span>Submit</span>
                </button>
                <button
                  x-show="step < 2"
                  @click="step++"
                  class="
                    hidden
                    w-32
                    focus:outline-none
                    border border-transparent
                    py-2
                    px-5
                    rounded-lg
                    shadow-sm
                    text-center text-white
                    bg-blue-500
                    hover:bg-blue-600
                    font-medium
                  "
                  id="realNextBtn"
                >
                  Next
                </button>

                <button
                  x-show="step === 2"
                  class="
                    w-48
                    inline-flex 
                    justify-center
                    focus:outline-none
                    border border-transparent
                    py-2
                    px-5
                    rounded-lg
                    shadow-sm
                    text-center text-white
                    bg-blue-500
                    hover:bg-blue-600
                    font-medium
                  "
                  id="fakeCompleteBtn"
                >
                <svg class="animate-spin -ml-1 mr-3 h-5 w-5 text-white hidden" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24">
                  <circle class="opacity-25" cx="12" cy="12" r="10" stroke="currentColor" stroke-width="4"></circle>
                  <path class="opacity-75" fill="currentColor" d="M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z"></path>
                </svg>
                <span>Complete</span>
                </button>

                <button
                  @click="step = 'complete'"
                  x-show="step === 2"
                  class="
                    hidden
                    w-32
                    focus:outline-none
                    border border-transparent
                    py-2
                    px-5
                    rounded-lg
                    shadow-sm
                    text-center text-white
                    bg-blue-500
                    hover:bg-blue-600
                    font-medium
                  "
                  id="realCompleteBtn"
                >
                  Complete
                </button>
              </div>
            </div>
          </div>
        </div>
        <!-- / Bottom Navigation https://placehold.co/300x300/e2e8f0/cccccc -->
      </div>
    </div>
  </main>
</div>
<script src="/js/skills.js"></script>
<script>
  function app() {
    return {
      step: 1,
      passwordStrengthText: "",
      togglePassword: false,

      image:
        "data:image/jpeg;base64,",
      password: "",
      gender: "Male",

      checkPasswordStrength() {
        var strongRegex = new RegExp(
          "^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#\$%\^&\*])(?=.{8,})"
        );
        var mediumRegex = new RegExp(
          "^(((?=.*[a-z])(?=.*[A-Z]))|((?=.*[a-z])(?=.*[0-9]))|((?=.*[A-Z])(?=.*[0-9])))(?=.{6,})"
        );

        let value = this.password;

        if (strongRegex.test(value)) {
          this.passwordStrengthText = "Strong password";
        } else if (mediumRegex.test(value)) {
          this.passwordStrengthText = "Could be stronger";
        } else {
          this.passwordStrengthText = "Too weak";
        }
      },
    };
  }
</script>
